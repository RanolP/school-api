{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    // "lib": [],
    "allowJs": false,
    "checkJs": false,
    "outDir": "./dist",

    "strict": true,

    "moduleResolution": "node",
    "esModuleInterop": true,

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
